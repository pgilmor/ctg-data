      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2017-03-01</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2017-03-08T18:07:45Z</lastSummaryUpdateDate>
          <text><![CDATA[<p><strong>Veterinary Medicine Loan Repayment Program Enhancement Act</strong></p> <p>This bill amends the Internal Revenue Code to exclude from gross income payments under the federal veterinary medicine loan repayment program or any state loan repayment or forgiveness program that is intended to provide for increased access to veterinary services in such state.</p>]]></text>
          <updateDate>2017-03-01T05:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
