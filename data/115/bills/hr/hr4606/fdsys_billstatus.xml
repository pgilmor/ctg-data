      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2017-12-11</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2018-04-26T16:35:25Z</lastSummaryUpdateDate>
          <text><![CDATA[<p><b>Ensuring Small Scale LNG Certainty and Access Act</b></p> <p>This bill amends the Natural Gas Act to state that an application under the Natural Gas Act for the importation or exportation of natural gas that does not exceed 0.14 billion cubic feet per day shall be granted without modification or delay.</p>]]></text>
          <updateDate>2017-12-11T05:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
