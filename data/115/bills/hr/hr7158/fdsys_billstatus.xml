      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2018-11-16</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2019-07-19T22:56:51Z</lastSummaryUpdateDate>
          <text><![CDATA[<p><b>Honoring All Veterans Act</b></p> <p>This bill adds the following as a mission statement for the Department of Veterans Affairs: &quot;To fulfill President Lincoln's promise to care for those 'who shall have borne the battle' and for their families, caregivers, and survivors.&quot;</p>]]></text>
          <updateDate>2018-11-16T05:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
