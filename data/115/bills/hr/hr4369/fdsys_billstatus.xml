      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2017-11-09</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2019-08-23T18:38:32Z</lastSummaryUpdateDate>
          <text><![CDATA[<p><b>Protecting Benefits for Disabled Veterans Act of 2017</b></p> <p>This bill authorizes the Department of Veterans Affairs to determine a veteran with a service-connected disability has a total disability based on his or her ability to secure or maintain substantially gainful employment as a result of the disability.</p>]]></text>
          <updateDate>2017-11-09T05:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
    </summaries>
