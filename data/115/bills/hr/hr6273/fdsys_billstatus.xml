      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2018-06-28</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2019-08-09T14:36:51Z</lastSummaryUpdateDate>
          <text><![CDATA[<p>This bill requires hospitals with emergency departments that participate in the 340B drug pricing program to receive, as a condition of continued participation, a specified certification relating to the availability of sexual assault forensic examiners at the hospital.</p>]]></text>
          <updateDate>2018-06-28T04:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
